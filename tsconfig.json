{
    "compileOnSave": true,
    "compilerOptions": {
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "isolatedModules": true,
        "jsx": "react",
        "lib": [
            "es6"
        ],
        "moduleResolution": "node",
        "noEmit": true,
        "strict": true,
        "target": "esnext",
        "noImplicitAny": true,
        "noImplicitThis": true,
        "suppressImplicitAnyIndexErrors": true,
        "experimentalDecorators": true,
        "strictNullChecks": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "importHelpers": true,
        "forceConsistentCasingInFileNames": true,
        "noImplicitReturns": true,
        "downlevelIteration": true,
        "module": "esnext",
        "baseUrl": ".",
        "paths": {
            // "*": ["src/*", "source/*"],
            // "test/*": ["test/*"],
            "@request/*": [
                "src/request/*"
            ],
            "@configure/*": [
                "src/configure/*"
            ],
            "@redux/*": [
                "src/redux/*"
            ],
            "@interface/*": [
                "src/interface/*"
            ],
            "@components/*": [
                "src/components/*"
            ],
            "@helps/*": [
                "helps/*"
            ],
            "@pages/*": [
                "src/pages/*"
            ],
            "@img/*": [
                "source/img/*"
            ],
            "@source/*": [
                "source/*"
            ],
            "@music/*": [
                "source/music/*"
            ],
            "underscore": [
                "lodash"
            ],
            "@local_modules/*": [
                "local_modules/*"
            ],
            "react-native-qj-fetch": [
                "local_modules/react-native-qj-fetch"
            ]
        }
    },
    "exclude": [
        "node_modules",
        "babel.config.js",
        "metro.config.js",
        "jest.config.js"
    ]
}